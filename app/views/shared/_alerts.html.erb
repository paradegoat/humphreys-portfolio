<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Geoff Humphreys", sticky: false) %>
  <% elsif flash[:error] %>
    <%= js add_gritter(flash[:error], title: "Geoff Humphreys", sticky: false) %>
    <% else flash[:notice] %>
      <%= js add_gritter(flash[:notice], title: "Geoff Humphreys", sticky: false) %>
<% end %>
